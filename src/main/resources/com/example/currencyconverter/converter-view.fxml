<VBox alignment="CENTER" prefHeight="236.0" prefWidth="744.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.currencyconverter.ConverterController">
